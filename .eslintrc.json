{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },

  "rules": {
    "prettier/prettier": ["warn", { "endOfLine": "auto" }],
    "no-unused-vars": "warn",
    "no-console": "off",
    "consistent-return": "off",
    "func-names": "off",
    "node/no-unpublished-require": "off",
    "import/no-extraneous-dependencies": "off",
    "global-require": "off",
    "no-undef": "off",
    "class-methods-use-this": "off"
  }
}
