{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },

  "rules": {
    "prettier/prettier": ["warn", { "endOfLine": "auto" }],
    "no-unused-vars": "warn",
    "no-console": "off",
    "consistent-return": "off",
    "func-names": "off"
  }
}
